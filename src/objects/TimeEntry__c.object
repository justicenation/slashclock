    <validationRules>
        <fullName>DurationCoversSlices</fullName>
        <active>true</active>
        <description>When a time entry is closed, make sure the duration covers all sliced time. It doesn&apos;t make sense for the total duration of time slices to exceed the time entry&apos;s duration.</description>
        <errorConditionFormula>AND(
  NOT( ISBLANK( EndTime__c ) ) ,
  DurationMinutes__c &lt; NumberOfSlicedMinutes__c
)</errorConditionFormula>
        <errorDisplayField>EndTime__c</errorDisplayField>
        <errorMessage>Duration cannot be shorter than the sum of all time slices</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EndTime</fullName>
        <active>true</active>
        <description>End Time must be after Start Time</description>
        <errorConditionFormula>IF(
  ISBLANK( EndTime__c ) ,
  FALSE ,
  EndTime__c &lt; StartTime__c
)</errorConditionFormula>
        <errorDisplayField>EndTime__c</errorDisplayField>
        <errorMessage>End Time must be after Start Time</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
